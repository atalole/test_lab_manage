{
  "name": "library-management-system",
  "version": "1.0.0",
  "description": "Backend API for managing library book inventory with wishlist notifications",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc",
  "verify": "npm run test",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "tsx node_modules/jest/bin/jest.js",
    "test:watch": "tsx node_modules/jest/bin/jest.js --watch",
    "test:coverage": "tsx node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "library",
    "management",
    "api",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.4.1",
    "bull": "^4.13.0",
    "ioredis": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "prisma": "^5.19.1",
    "nodemon": "^3.1.9",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "@types/jest": "^29.5.14",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "typescript": "^5.5.4",
    "tsx": "^4.16.2",
    "ts-jest": "^29.1.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}

